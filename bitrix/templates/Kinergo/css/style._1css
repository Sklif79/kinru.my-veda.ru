}

.tbl-equip tr:first-child td {border-top: none;}
.tbl-equip tr:last-child td {border-bottom: none;}
.tbl-equip tr td:first-child {font-weight: bold;}

.etext {
	padding-bottom: 35px;
	margin-bottom: 30px;
	background: url('../img/hr.gif') left bottom repeat-x;
}

.etext p {
	font-size: 13px;
	line-height: 18px;
}

.eblock {margin-bottom: 40px;}
.eblock h1, .eblock h2 {text-transform: none;}

.eblock .image {
	width: 192px;
	margin-right: 40px;
	display: inline-block;
	vertical-align: top;
}

.eblock .info p {padding-bottom: 20px; font-size: 13px; line-height: 18px;}

.eblock .info {
	width: 760px;
	display: inline-block;
	vertical-align: top;
}

.eblock .btn-more {
	display: inline-block;
	width: 135px;
	font: normal 13px Arial;
	color: white;
	padding: 7px 0;
	text-align: center;
	text-decoration: none;
	text-transform: uppercase;
	background-color: #13487c;
}

.btn-more:hover {background-color: #10569b;}

.text-block .special-offer {background: none; padding: 0; margin: 0;}

.ec h2 {text-transform: none;}

.ec ul {
	list-style: none inside;
	padding-bottom: 40px;
}

.ec li {

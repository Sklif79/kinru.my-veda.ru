	height: 33px;
	text-align: center;
	color: white;
	background-color: #124677;
	text-decoration: none;
	font-size: 16px;
}

.btn-home {
	height: 27px;
	padding-top: 6px;
}

.equipment .gridl {
	width: 310px;
	display: inline-block;
	margin-right: 25px;
	vertical-align: top;
}

.equipment .gridr {
	width: 665px;
	display: inline-block;
	vertical-align: top;
}

.equipment .gridl .image {
	text-align: center;
	margin-bottom: 50px;
}

.equipment .gridl .video a {
	display: inline-block;
	font-size: 13px;
	color: #221f1f;
